<configuration name="xml_curl.conf" description="cURL XML Gateway">
  <bindings>
    <binding name="directory">
      <param name="timeout" value="10" />
      <param name="gateway-url" value="http://$${FS_AS_IP}:$${AS_PORT}/Api/Fs/directory" bindings="directory" />
    </binding>
    <binding name="dialplan">
      <param name="timeout" value="10" />
      <param name="gateway-url" value="http://$${FS_AS_IP}:$${AS_PORT}/Api/Fs/dialplan" bindings="dialplan" />
    </binding>
  </bindings>
</configuration>
